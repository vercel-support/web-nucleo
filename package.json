{
  "name": "web-nucleo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3005",
    "dev-real": "USE_REAL_DATA=true next dev -p 3005",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 USE_REAL_DATA=true next build && rm -r .next/temp",
    "start": "cross-env NODE_ENV=production next start -p 3005",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint-fix": "eslint . --ext ts --ext tsx --ext js --fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn type-check"
    }
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.13",
    "@sentry/react": "^5.23.0",
    "@sentry/tracing": "^5.23.0",
    "@types/googlemaps": "^3.39.13",
    "axios": "^0.19.2",
    "blueimp-md5": "^2.17.0",
    "globby": "^11.0.1",
    "imagemin-svgo": "^8.0.0",
    "next": "^9.5.0",
    "react-google-maps": "^9.4.5",
    "react-swipeable": "^5.5.1",
    "sharp": "^0.25.4",
    "wpapi": "^1.2.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.2.1",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@types/blueimp-md5": "^2.7.0",
    "@types/gtag.js": "^0.0.3",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-responsive": "^8.0.2",
    "@types/styled-components": "^5.1.0",
    "@types/wpapi": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.3.5",
    "async-retry-decorator": "^1.3.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "image-trace-loader": "^1.0.2",
    "imagemin-optipng": "^8.0.0",
    "jsforce": "^2.0.0-alpha.12",
    "less": "^3.11.3",
    "less-vars-to-js": "^1.3.0",
    "next-optimized-images": "^2.6.1",
    "null-loader": "^4.0.0",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-responsive": "^8.1.0",
    "rosetta": "^1.1.0",
    "styled-components": "^5.1.1",
    "swr": "^0.2.3",
    "typescript": "^3.9.5",
    "webp-loader": "^0.6.0"
  },
